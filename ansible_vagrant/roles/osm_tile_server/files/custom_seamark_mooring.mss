/* #seamark[type='mooring'][seamark:mooring:category='pile'] */
#seamark-moorings {
  [category = 'dolphin'] {
    [zoom >= 15] {
      marker-file: url('symbols/custom_seamark_mooring/dolphin.png');
      marker-width: 1;
      marker-height: 1;
      marker-allow-overlap: true;
    }
    [zoom >= 16] {
      marker-file: url('symbols/custom_seamark_mooring/dolphin.png');
      marker-width: 2;
      marker-height: 2;
      marker-allow-overlap: true;
    }
    [zoom >= 17] {
      marker-file: url('symbols/custom_seamark_mooring/dolphin.png');
      marker-width: 3;
      marker-height: 3;
      marker-allow-overlap: true;
    }
    [zoom >= 18] {
      marker-file: url('symbols/custom_seamark_mooring/dolphin.png');
      marker-width: 4;
      marker-height: 4;
      marker-allow-overlap: true;
    }
    [zoom >= 19] {
      marker-file: url('symbols/custom_seamark_mooring/dolphin.png');
      marker-width: 10;
      marker-height: 10;
      marker-allow-overlap: true;
    }
  }
  [category = 'bollard'] {
    [zoom >= 15] {
      marker-file: url('symbols/custom_seamark_mooring/bollard.png');
      marker-width: 1;
      marker-height: 1;
      marker-allow-overlap: true;
    }
    [zoom >= 16] {
      marker-file: url('symbols/custom_seamark_mooring/bollard.png');
      marker-width: 2;
